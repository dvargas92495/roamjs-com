    "@babel/preset-react": "^7.12.1",
    "@types/aws-lambda": "^8.10.61",
    "@types/color": "^3.0.1",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node-emoji": "^1.8.1",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.7.0",
    "http-server": "^0.12.3",
    "jest": "^26.4.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
