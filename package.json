{
  "name": "roam-js-extensions",
  "version": "1.0.0",
  "description": "Various Roam Javascript extensions deployed to roamjs.com",
  "main": "index.js",
  "scripts": {
    "automate": "node scripts/automation.js",
    "build": "webpack --config ./webpack.production.config.js",
    "build:pr": "webpack --config ./webpack.development.config.js",
    "build:api": "webpack --config ./webpack.lambda.config.js --env.dir=api",
    "build:automations": "webpack --config ./webpack.lambda.config.js --env.dir=automations",
    "lambda": "node out/debug-lambda.js",
    "presetup:local": "npm run build",
    "setup:local": "node scripts/prelocal.js",
    "prelocal": "npm run setup:local",
    "local": "http-server",
    "prelambda": "webpack --config ./webpack.debug.config.js",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvargas92495/roam-js-extensions.git"
  },
  "keywords": [
    "Roam"
  ],
  "author": "dvargas92495",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dvargas92495/roam-js-extensions/issues"
  },
  "homepage": "https://github.com/dvargas92495/roam-js-extensions#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.61",
    "@types/node-emoji": "^1.8.1",
    "babel-preset-env": "^1.7.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.7.0",
    "http-server": "^0.12.3",
    "jest": "^26.4.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@testing-library/dom": "^7.22.6",
    "@testing-library/user-event": "^12.1.3",
    "axios": "^0.20.0",
    "date-fns": "^2.16.1",
    "mobile-device-detect": "^0.4.3",
    "node-emoji": "^1.10.0",
    "roam-client": "^1.6.1",
    "tesseract.js": "^2.1.3"
  }
}
